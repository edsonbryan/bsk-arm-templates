{
  "$schema": "http://schemas.microsoft.org/azure/deploymentTemplate?api-version=2015-01-01-preview#",
    "contentVersion": "1.0",
    "parameters": {
        "accountName": {
          "type": "string"
        },
        "scriptUri": {
          "type": "string"
        },
        "scriptSas": {
          "type": "string"
        }
    },
    "variables": {
        "runbookName": "DeploymentHistoryCleanup",
        "sku": "Free",
        "scriptUri" : "[concat(parameters('scriptUri'), parameters('scriptSas'))]"
    },
    "resources": [
        {
            "type": "Microsoft.Automation/automationAccounts",
            "name": "[parameters('accountName')]",
            "apiVersion": "2015-01-01-preview",
            "location": "[resourceGroup().location]",
            "dependsOn": [],
            "tags": {},            
            "properties": {
              "sku": {
                "name" : "[variables('sku')]"
              }
            },
            "resources": [
              {
                "name": "[variables('runbookName')]",
                "type": "runbooks",
                "apiVersion": "2015-01-01-preview",
                "location": "[resourceGroup().location]",
                "dependsOn": [
                  "[concat('Microsoft.Automation/automationAccounts/', parameters('accountName'))]"
                ],
                "tags": {},
                "properties": {
                  "runbookType": "PowerShell",
                  "logProgress": false,
                  "logVerbose": false,
                  "description": "",
                  "publishContentLink": {
                    "uri": "[variables('scriptUri')]",
                    "version": "1.0.0.0"
                  }
                }
              }
            ]
        }
    ],
    "outputs": {}
}